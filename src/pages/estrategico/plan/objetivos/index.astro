---
import ObjetivosPage from "@/process/strategic/dashboard/plan/ObjetivoPage";
import PlanLayout from "@/process/strategic/layouts/PlanLayout.astro";
import "@/styles/global.css";

const title = "Dashboard - Gestion de Objetivos";
const description = "Panel de objetivos por plan con modal de KPIs detallados";
const robots = ["noindex", "nofollow"];
const keywords = [
  "INCADEV",
  "estrategico",
  "objetivos",
  "dashboard",
  "gestion",
];
const url =
  "https://instituto.cetivirgendelapuerta.com/estrategico/plan/objetivos";
const imageOG = "/ISOLOGOTIPO_VERTICAL.svg";
const imagetwt = "/ISOLOGOTIPO_VERTICAL.svg";
---

<PlanLayout
  {robots}
  {title}
  {description}
  {keywords}
  {url}
  {imageOG}
  {imagetwt}
>
  <ObjetivosPage client:load />
</PlanLayout>
